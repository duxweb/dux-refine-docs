{
  "name": "@islandjs/plugin-search",
  "version": "0.7.31",
  "description": "Search plugin for islandjs",
  "packageManager": "pnpm@7.9.2",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/*": "./dist/*",
    "./package.json": "./package.json"
  },
  "keywords": [
    "vite",
    "ssg",
    "island architecture",
    "docs",
    "framework"
  ],
  "author": "Sanyuan0704",
  "license": "MIT",
  "devDependencies": {
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@unocss/preset-attributify": "^0.45.26",
    "react": "^18.2.0"
  },
  "peerDependencies": {
    "islandjs": "0.7.31"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,json}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "flexsearch": "0.6.32",
    "lodash-es": "^4.17.21"
  },
  "scripts": {
    "pnpm-install": "pnpm install",
    "dev": "tsup --watch --format=cjs,esm",
    "build": "cross-env NODE_ENV=production tsup --format=cjs,esm",
    "test": "vitest run"
  }
}